}

.history-more .data{
	color: #B0B0B0;
	float: right;
	margin-right: 5px;
	line-height: 60px;
}

.history-item .item-lable{
	margin-left: 25px;
}

.history-item .item-right{
	float: right;
	margin-right: 25px;
}

.history-item .right{
	margin-right: 25px;
}

.history-more .check, .select-more .check{
	height: 40px;
	line-height: 40px;
	margin-left: 55px;
	position: relative;
}

.history-more .alone-img, .select-more .alone-img{
	height: 20px;
	position: absolute;
	top: 10px;
}

.history-more .check span, .select-more .check span{
