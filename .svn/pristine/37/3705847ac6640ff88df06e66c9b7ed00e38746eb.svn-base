/* 窗口 */

.win{
	margin: 30px 25px;
}
.win .content{
	height: 100%;
	border-radius: 8px 8px 0 0;
	overflow: hidden;
	background: #EFF4FB;
	margin-top: 50px;
}

.win .content .title{
	height: 40px;
	background: #6196DA;
}

.win .content .title span{
	float: right;
	margin-right: 25px;
	border: 1px #fff solid;
	color: #fff;
	height: 20px;
	padding: 5px 15px;
	border-radius: 8px;
	margin-top: 4px;
}

.win .content .lable{
	margin-left: 25px;
}
.win .content .input-text{
	background: #EFF4FB;
	margin-right: 20px;
}

.win .item li{
	height : 60px;
	border-bottom: 1px solid #DEDCDC;
	line-height: 60px;
}

.win .btn-add{
	margin-top: 50px;
}
.win .btn-add .btn-border{
	text-align : center;
	width : 40%;
	margin: 0px auto;
	height: 50px;
	border-radius: 8px;
	border: 3px dashed #E0E0E0;
}

.win .btn-add .btn-border .btn-click{
	height : 40px;
	margin : 5px 5px;;
	background :#E0E0E0;
	border-radius: 8px;
	position: relative;
}

.win .btn-add .btn-border .btn-click .img{
	height: 20px;
	position: relative;
	top:10px;
}

.win .btn-add .btn-border .btn-click span{
	display: inline-block;
	position: relative;
	top:5px;
}

.win .jt{
	float: right;
	margin-top: 22px;
	margin-right: 15px;
	height: 20px;
}

.win .data{
	color: #666666;
	margin-right: 10px;
	float: right;
}

.win .item .family-select{
	height: 100%;
}

.win .item .family-select .more-check{
	background: #EFF4FB;
}

.win .return, .win .btn-save{
	float: left;
	width: 30%;
	text-align: center;
	margin-top: 50px;
	background: #CD412F;
	margin-left: 12%;
	height: 30px;
	border-radius: 8px;
	padding-top: 10px;
	color: #fff;
	margin-bottom: 100px;
}

.win .more-check{
	background: #EFF4FB;
}



