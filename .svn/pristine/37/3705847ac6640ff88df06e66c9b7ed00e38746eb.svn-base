}

.life-more .title{
	line-height: 60px;
	padding-left: 35px;
}

.life-more .data{
	color: #B0B0B0;
	float: right;
	margin-right: 5px;
	line-height: 60px;
}

.active-base .select-more .right{
	margin-right: 5px;
}

.life-item .item-lable{
	margin-left: 35px;
}

.life-item .item-right{
	float: right;
	margin-right: 25px;
}

.life-item .right{
	margin-right: 25px;
}

.more-check .more-item{
	height: 30px;
	width: 10%;
	display : inline-block;
	margin: 16px 0px;
	font-size: 16px;
	border-radius: 10px;
	line-height: 40px;
	text-align: center;
	margin-left: 3%;
	padding: 0px 10px;
	position: relative;
}

.more-item .check-img{
	width: 20px;
	position: absolute;
	top: 10px;
	left: 30px;
}

.more-item .opt{
	display: inline-block;
	position: absolute;
	left: 60px;
}

/* 结束 */

.before{
	float: left;
	margin-left: 13%;
	color: red;
	background: #fff;
	border: 2px solid #CD412F;
	font-weight: bold;
}

.after{
	float: left;
	margin-left: 13%;
	font-weight: bold;
}

/* 开始 */
