* {
	margin: 0px;
	padding: 0px;
	font-family: Microsoft YaHei;
}

html,body{-webkit-text-size-adjust: 100%;-webkit-tap-highlight-color: rgba(0, 0, 0, 0);}

.core{
	width: 100%;
	height: 100%;
	margin: 0px;
	padding: 0px;
}

.font-def1{
	font-size: 16px !important;
}

.nav {
	display: flex;
	height: 80px;
	background: #F0F0F0;
	position: relative;
}

.nav .tab {
	flex-grow: 1;
	text-align: center;
	font-size: 15px;
	padding-top: 47px;
}

.nav .line{
	display: flex;
	position:absolute;
	width:100%;
	border-top:1px solid #DEDCDC;
	top: 14px;
}
.nav .spot{
	display: flex;
	position:absolute;
	width: 100%;
	text-align: center;
}

.spot div{
	flex-grow: 1;
	margin-top: 15px;
	z-index: 2;
}

.tab-select-img{
	margin-top: 4px;
	width: 20px;
	height: 20px;
}

.tab-select{
    color: #6196DA;
}

.tab-img{
	width: 15px;
	height: 15px;
	margin-top: 7px;
}

.active-base li{
	list-style: none;
	height: 60px;
	border-bottom: 1px solid #DEDCDC;
	line-height: 60px;
}

.active-base .title{
	font-size: 17px;
	color: #545454;
}

.active-base .lable{
	margin-left: 25px;
}

.active-base .title-right{
	float:right;
	font-size: 16px;
	color: #545454;
	margin-right: 29px;
}

.active-base .data{
	font-size: 17px;
	float: right;
	color: #666666;
	margin-right: 29px;
}

.active-base .jt{
	float: right;
	margin-top: 22px;
	margin-right: 25px;
	height: 20px;
}

.select-more .data{
	margin-top: 24px;
	color: #B0B0B0;
}

.input-text {
	outline: none;
	border: none;
	float: right;
	width: 70%;
	margin: 0px 10px;
	height: 100%;
	font-size: 16px;
	padding: 0px;
}

.nouint{
	margin-right: 25px;
}

.active-base .select-more{
	height:100%;
	line-height: 100%;
}
.active-base .title{
	line-height: 60px;
}
.more-check{
	clear:both;
	background: #F0F0F0;
	display: none;
	position: relative;
}

.check-item{
	height: 40px;
	width: 10%;
	border: 1px solid #CCCCCC;
	display : inline-block;
	margin: 16px 0px;
	font-size: 16px;
	border-radius: 10px;
	line-height: 40px;
	text-align: center;
	background:#ffffff;
	margin-left: 3%;
	padding: 0px 10px;
}

.select-item{
	background:#CD412F;
	color: #fff;
}


.select-more .base-btn{
	height: 100px;
	background: #F0F0F0;
	padding: 1px;
	display: none;
}
.select-more .base-btn .base-dashed{
	width: 45%;
	border: 3px #E0E0E0 dashed;
	height: 40px;
	border-radius: 8px;
	margin: 25px auto;
	padding: 5px;
}

.select-more .base-dashed .backgd{
	background: #E0E0E0;
	height: 100%;
	border-radius: 8px;
	text-align: center;
	position: relative;
}

.select-more .base-dashed .backgd div{
	display: flex;
	justify-content: center;
}

.select-more .base-dashed .backgd .add-img{
	
}
.select-more .base-dashed .backgd .desc{
	
}


