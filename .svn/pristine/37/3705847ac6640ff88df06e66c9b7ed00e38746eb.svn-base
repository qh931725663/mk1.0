	position: relative;
	display: flex;
	justify-content: center;
}

.backgd .add-img{
	width: 28px;
	height: 28px;
	margin: 6px 2px;
	/* position: absolute;
	top:6px;
	left:22% */
}
.backgd .desc{
	display:inline-block;
	margin: 11px 2px;
	/* position: absolute;
	top:12px;
	left:33% */
}

.history-input{
	background: #F0F0F0;
	margin-top: 20px;
	margin-right: 25px;
}
