<script>
$(function(){
    //初始化加载显示数据

    //获取地址栏order_id方法
	function getUrlParam(name) {
		var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)");
		var r = window.location.search.substr(1).match(reg);
		if (r != null) return unescape(r[2]);
			return null;
		}
	var order_id=getUrlParam("order_id");
	var user_id=getUrlParam("user_id");
	$("#order_id").val(order_id);
	$("#user_id").val(user_id);
    init(order_id);
    //数据展示
    function init(order_id){
        $.ajax({
            url:"../../../plan/planItem.action",
            type:"POST",
            dataType:'json',
            data:{order_id:order_id},
            success:function(response){
                if(response.status=="1") {
                	var html="";
                    var tr_table="";
                    var total_tr="";
                    $.each(response.data, function (i, p) {
	                    var tr_list="";
						$.each(p.itemList, function(v,k){
							tr_list+='<tr>'+
	                            '<td>'+(v+1)+'</td>'+
	                            '<td>'+ k.plan_name+'</td>'+
	                            '<td>'+ k.plan_content+'</td>'+
	                            '<td>暂无</td>'+
	                            '<td>'+ k.status+'</td>'+
	                            '<td><button class="btn btn-danger radius" onclick="track('+k.order_id+','+k.plan_item_type+',$(this).parent().parent().children().eq(1).html())" style="margin-right: 5px;'+(k.plan_item_type=='1'?'display:none':k.plan_item_type=='4'?'display:none':'')+'" >追踪</button><button onclick="detail('+k.order_id+',$(this).parent().parent().children().eq(1).html())" class="btn btn-success radius">查看</button></td>'+
	                            '</tr>';
						});
                         tr_table='<tbody>'+tr_list+'</tbody>';
                          html+= '<tr>'+
                                '<td>'+p.plan_item_name+'</td>'+
                                '<td>'+p.level_name+'</td>'+
                                '<td>'+p.status+'</td>'+
                                '<td><button class="btn btn-primary btn-user-edit radius grade-two-list"  type="button">展开</button></td>'+
                                '</tr>'+
                                '<tr class="toggle-list" style="display:none;">'+
	                                '<td colspan="5">'+
	                                '<table class="table table-border table-bordered" style="margin: 10px 10px;width: 85%;">'+
	                                	'<thead style="background: #ddd">'+
	                                		'<tr class="text-c"><th>序号</th><th>管理内容</th><th>常规管理</th><th>当前完成次数</th><th>状态</th><th>操作</th></tr>'+
	                                	'</thead>'+tr_table+'</table>'+
	                				'</td>'+
            					'</tr>';
                    });
                    $(".plan-tbody").append(html);
                }
            }
        }).done(function(){
        	$("button.grade-two-list").click(function(){
    			$(this).parent().parent("tr").next("tr.toggle-list").toggle();
			});	
        });
    }
});
//总的展开
$("#total-toggle").click(function(){
    $("tr.toggle-list").toggle();
});

