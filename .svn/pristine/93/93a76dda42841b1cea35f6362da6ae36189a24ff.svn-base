//追踪保存
	$(".btn-save").click(function(){
		var _self=this;
		$(_self).next("button.layui-layer-close").click();
		var obj_list={};
		var data_list=[];
		//表单
		$(_self).parent().parent("div.input-answer").find("div.input-list").each(function(){
	 		var label=$(this).find("label").html();
	 		var val=[];
            var span_length=$(this).find("span.company").length;
            if(span_length==1){
                var span_html=$(this).find("span.company").html();
            }else{
                var span_html="";
            }
            obj_list.option_unit=span_html;	 	
            	
	 		$(this).find(".input-list-1,.input-list:checked").each(function(){
	 			val.push($(this).val());
	 		});
	 		var val_str=val.join(',');
	 		obj_list.option_name=label;
	 		obj_list.option_value=val_str;
            obj_list.option_unit=span_html;
            data_list.push(obj_list);
	 		obj_list={};
 		});		
 	 	var dataList={
			plan_order_id:$("#order_id").val(),
			dict_order_id:$("#order_id_zuizong").val(),
			user_id:$("#user_id").val(),
			data:data_list
		}
 	  	$.ajax({
	    	url:"../../../plan/addTrackItem.action",
	    	type:"post",
	    	dataType:'json',
	    	data:dataList,
	    	success:function(response){
	    		if(response.status=="1"){
	    			 layer.msg('追踪成功!');
	    			 $(_self).next("button.layui-layer-close").click();
	    			 $(_self).parent().parent("div.input-answer").find(":input").each(function(){
	    			 	$(this).val("");
	    			 });
	    		}else{
	    			 layer.msg('追踪失败!');
	    		}
	    	}
	    });
	   
	});         
