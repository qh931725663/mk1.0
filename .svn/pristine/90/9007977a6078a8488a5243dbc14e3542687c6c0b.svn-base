<!--请在下方写此页面业务相关的脚本-->
<script type="text/javascript" src="../lib/My97DatePicker/WdatePicker.js"></script>
<script type="text/javascript" src="../lib/datatables/js/jquery.dataTables.min.js"></script>
<script type="text/javascript" src="../lib/laypage/laypage.js"></script>
<script type="text/javascript">
$(function(){
	//医生管理列表初始化加载
	getDataList(0,null);

	//条件查询
	var initFlag2="";
	$(".btn-search").click(function(){
		initFlag2=true;
		getDataList(0,null);
	});
	var initFlag = true;
	//查询函数
	function getDataList(currPage, jg) {
		var data=$.param({pageno :currPage+1})+'&'+ $('#form-doctor-search').serialize();
		$.ajax({
			url : "../../../user-platform/doctorgridquery.action",
			type : "post",
			dataType : 'json',
			data : data,
			success : function(response) {
				console.log(response);
				var page_count=response.recordCount;
				if (initFlag) {
					//分页插件
					$("#Pagination").pagination(page_count,{
						callback : getDataList//回调函数
					});
					initFlag = false;
				}
				if (initFlag2) {
					//debugger;
					$("#Pagination").pagination(page_count,{
						callback : getDataList//回调函数
					});
					initFlag2 = false;
				}
				//调用数据展示函数
				show_list(response);
			}
		});
	}

	//数据列表展示公共函数
	function show_list(response){
		var html = '';
		var pn=response.pageNo;
		var num=(pn-1)*10+1;
		$.each(response.data, function(i,p){
			html +=`<tr class='text-c'>
			<td>`+(num+i)+`</td>
			<td>${p.user_name}</td>
			<td>${p.user_id}</td>
			<td>${p.user_card}</td>
			<td>${p.user_phone}</td>
			<td>${p.user_sex}</td>
			<td>${p.user_age}</td>
			<td>${p.doctor_title}</td>
			<td>${p.hosp_name}</td>
			<td>${p.department_name}</td>
			<td>${p.ceate_date}</td>
			<td>
			<span onclick="doctor_edit(${p.user_id});"  style="width:75px;color:#da281f;cursor: pointer;">编辑</span>
			</td>
			</tr>`;
		});
		$(".doctor-tablelist .doctor-tbody").html(html);
		$(".page-count").html(response.pageCount);
	}

});
/*医生编辑*/
function doctor_edit(user_id){
	layer_show('医生编辑','doctor-edit.html?'+user_id,'100','100');

}
/*增加医生*/
function doctor_add(title,url,w,h){
	layer_show(title,url,w,h);
}

$(function(){
	//用户批量上传
	$(".doctor_batch_upload").click(function(){
		index_layer_receivable_fund=layer.open({
			type: 1,
			area: ['450px', '250px'],
			title: false,
			content:$('#layer_doctor_batch_upload')
		});
	});
	//数据导入
	$("#upload-file").click(function(){
		if($("#uploadfile").val()!=""){
			setTimeout(function(){
				layer.close(index_layer_receivable_fund);
				index_layer_upload_mes=layer.open({
						type: 1,
						area: ['300px', '200px'],
						title: false,
						content: $("#layer_batch_upload_mes")
					});	
			},10);
			$.ajax({
			    url : "../../../batch/read.action",
			    type : "post",
			    dataType : 'json',
			    timeout:55000,
			    data :{path:"D:/memberexcel.xlsx",data_type:''},
			    success : function(response) {
			    	$("#layer_mes").html(response.data);
			    },
			    error: function (XmlHttpRequest, textStatus, errorThrown) {
					//alter(data);
					$("#layer_mes").html("上传失败！");
				}	
			}); 
		}		
	});
});
</script>
</body>
</html>