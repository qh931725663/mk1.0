	//回到顶部
	var oTop = $('#goTop'); //回到顶部
	$(window).scroll(function() {
		//可视窗口高度
		var winH = $(window).height();
		//鼠标滚动的距离
		var iTop = $(window).scrollTop();
		if(iTop >= $("#am1").height()) {
			oTop.fadeIn();
		} else {
