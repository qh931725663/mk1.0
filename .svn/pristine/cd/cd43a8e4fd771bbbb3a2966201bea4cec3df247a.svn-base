	//家庭成员 既往史 手术史 用药史
	function adoption_list(obj,lish){
		$(obj).click(function(){
			$(lish).each(function(){
				if($(obj).attr("data-name")=="no"){
					$(this).hide();
				}else{
					$(this).show();
					$(lish).find("input[type='checkbox']").each(function(){
						$(this).attr("checked",false);
						$(this).find("input.clear").val("");
					});
					$(lish).find("input.clear").each(function(){
						$(this).val("");
					});
				}
			});
		});
	}
