.position-container{position: fixed;right:-115px;top:80px;background:#ea5e4e;z-index: 1000}
.position-nav{width:115px;text-align: center;}
.position-container .btn-show-show{display: inline-block;position: relative;color: #fff;left: -35px;top:204px;background:#ea5e4e;width: 35px;height:28px;line-height: 28px;font-size: 20px;}
.position-container .btn-show-hidden{display: inline-block;position: relative;left: -36px;top:204px;background:#ea5e4e;width: 30px;height:28px;line-height: 28px;color: #fff;font-size: 20px;}
.position-nav li.na-menu{border-bottom: 1px solid #fff;font-size: 16px;}
.position-nav li,.position-nav li a{color:#fff;text-decoration: none;}
.position-nav li{margin-bottom: 8px;width:115px;text-align: center;}
