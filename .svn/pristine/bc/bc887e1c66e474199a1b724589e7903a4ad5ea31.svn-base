<script>
$(function(){
    //初始化加载显示数据
    init();
    //数据展示
    function init(){
        $.ajax({
            url:"../../../plan/planItem.action",
            type:"get",
            dataType:'json',
            data:{order_id:9},
            success:function(response){
                if(response.status=="1") {
                	var html="";
                    var tr_table="";
                    var total_tr="";
                    $.each(response.data, function (i, p) {
	                    var tr_list="";
						$.each(p.itemList, function(v,k){
							tr_list+='<tr>'+
	                            '<td>'+(v+1)+'</td>'+
	                            '<td>'+ k.plan_name+'</td>'+
	                            '<td>'+ k.plan_content+'</td>'+
	                            '<td>暂无</td>'+
	                            '<td>'+ k.status+'</td>'+
	                            '<td><button class="btn btn-danger radius" onclick="track('+k.order_id+','+k.plan_item_type+',$(this).parent().parent().children().eq(1).html())" style="margin-right: 5px;'+(k.plan_item_type=='1'?'display:none':k.plan_item_type=='4'?'display:none':'')+'" >追踪</button><button class="btn btn-success radius">查看</button></td>'+
	                            '</tr>';
						});
                         tr_table='<tbody>'+tr_list+'</tbody>';
                          html+= '<tr>'+
                                '<td>'+p.plan_item_name+'</td>'+
                                '<td>'+p.level_name+'</td>'+
                                '<td>'+p.status+'</td>'+
                                '<td><button class="btn btn-primary btn-user-edit radius grade-two-list"  type="button">展开</button></td>'+
                                '</tr>'+
                                '<tr class="toggle-list" style="display:none;">'+
	                                '<td colspan="5">'+
	                                '<table class="table table-border table-bordered" style="margin: 10px 10px;width: 85%;">'+
	                                	'<thead style="background: #ddd">'+
	                                		'<tr class="text-c"><th>序号</th><th>管理内容</th><th>常规管理</th><th>当前完成次数</th><th>状态</th><th>操作</th></tr>'+
	                                	'</thead>'+tr_table+'</table>'+
	                				'</td>'+
            					'</tr>';
                    });
                    $(".plan-tbody").append(html);
                }
            }
        }).done(function(){
        	$("button.grade-two-list").click(function(){
    			$(this).parent().parent("tr").next("tr.toggle-list").toggle();
			});	
        });
    }
});
//总的展开
$("#total-toggle").click(function(){
    $("tr.toggle-list").toggle();
});

function track(order_id,item_type,plan_name){
    if(item_type==2 && plan_name=='饮食指导'){//药物治疗
        index_layer_addseller_2_1=layer.open({
            type: 1,
            title:'饮食指导',
            area: ['550px', '350px'],
            content:$('#layer_addseller_2_1')
        });
    }
    if(item_type==2 && plan_name=='运动监测'){//药物治疗
        index_layer_addseller_2_3=layer.open({
