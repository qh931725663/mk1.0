<script src="../../../js/jquery-1.4.2.min.js"></script>
<script src="js/jquery.step.js"></script>
<script>
    $(function() {
        $("#save").click(function(){
            var data=$.param({order_id: 9,scope_flag:1})+'&'+ $('#form-baseinfo-write').serialize();
            $.ajax({
                url:"../../../diabetesDoc-app/addAnswers.action",
                type:"post",
                dataType:'json',
                data:data,
                success:function(response){
                    if(response.status=="1"){
                        alert("保存成功！");
                    }else{
                        alert("添加失败！");
                    }
                }
            });
        });
         //选择隐藏显示
        $(".bas-close").click(function(){
            $(this).parents(".step2-list-ul").next(".data-show-or-hide").hide();
        });
        $(".bas-open").click(function(){
            $(this).parents(".step2-list-ul").next(".data-show-or-hide").show();
        });
        //选择时间 触发时间默认清空
        $(".date").focus(function(){
            $(this).attr({'placeholder':''});
        });
        //上一步 下一步
        var step= $("#myStep").step();
        $("#preBtn").click(function(event) {
            var yes=step.preStep();//上一步
            $("#nextBtn").show();
            $("#save").hide();

        });
        $("#nextBtn").click(function(event) {
            var yes;
            $("#nav-title li").each(function(index){
                var istrue=$(this).hasClass("step-active");
                var val=$(this).attr("data_obj");
                if(istrue){
                    if(val==0){
                        yes=step.nextStep();
                        //校验 FALSE 停止 true执行 var yes=step.nextStep();
/*                        if(false){
                            //return false;
                            var yes=step.nextStep()
                        }else{
                            return false;
                        }*/
                        return false;
                    }
                    if(val==1){
                        yes=step.nextStep();
                        return false;
                        //alert(val)
                    }
                    if(val==2){
                        yes=step.nextStep();
                        $("#nextBtn").hide();
                        $("#save").show();
                    }
                }

            });

        });
    });
</script>
