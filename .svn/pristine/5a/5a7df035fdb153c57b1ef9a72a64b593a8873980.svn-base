<!--请在下方写此页面业务相关的脚本-->
<script type="text/javascript" src="../lib/jquery.validation/js/jquery.validate.js"></script>
<script type="text/javascript" src="../lib/jquery.validation/js/validate-methods.js"></script>
<script type="text/javascript">
    var menucode="";
    $(function(){
        //获取地址栏后面菜单参数
        var a = window.location.href;
        var s=a.indexOf("?");
        menucode=a.substring(s+1);
        console.log(menucode);

        //上传身份证组件
        function upload_img(up_img,show_img){
            $(up_img).change(function() {
                var file = this.files[0];
                var r = new FileReader();
                r.readAsDataURL(file);
                $(r).load(function() {
                    $(show_img).attr({src:this.result,class:'no-bor'});
                })
            })
        }
        upload_img("#image","#look_img");
        upload_img("#image2","#look_img2");

    });
    //新增会员用户
    function member_add(){
        debugger;
        member_add_check();
        if(member_add_flag){
            var data=$.param({c_menu_code:menucode})+'&'+ $('#form-datacollection-add').serialize();
            console.log("11"+data);
            $.ajax({
                url: "../../../user-platform/add.action",
                type: "post",
                dataType: 'json',
                data:data,
                success: function (response) {
                    debugger;
                    console.log(response);
                    user_id = response.data.user_id;
                    upload_image(user_id);
                    layer.msg('成功添加!');
                    //页面8秒钟后关闭
                    setTimeout(function () {
                        parent.location.reload(); // 父页面刷新
                        parent.layer.close(index1);
                    }, 1000);
                }
            });
            var index1 = parent.layer.getFrameIndex(window.name);
        }
    }

    //上传图片
    function upload_image(user_id){
        debugger;
        console.log(user_id);
        var data=new FormData($("#form-datacollection-add")[0]);
        $.ajax({
            type:"post",
            url:"../../../upload/file.action",
            headers:{user_id:user_id,report_type:'500006'},
            data:data,
            dataType:"json",
            processData:false,
            contentType:false,
            success:function(data){
                console.log(data);
                files = data;
            }
        });
    }

</script>
<!--/请在上方写此页面业务相关的脚本-->
</body>
</html>