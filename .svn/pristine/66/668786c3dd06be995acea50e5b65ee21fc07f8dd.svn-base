//添加医生
function do_save_jy(){
    var data = $('#form-doctor-add').serialize();
    $.ajax({
        url: "../../../user-platform/addDoctor.action",
        type: "post",
        dataType: 'json',
        data: data,
        success: function (response) {
            console.log(response);
            layer.msg('成功添加!');
            setTimeout(function () {
                parent.location.reload(); // 父页面刷新
                parent.layer.close(index1);
            }, 1000);
        }
    });
    var index1 = parent.layer.getFrameIndex(window.name);
}
//新增医生用户
function do_save(){
    doctor_add_check();
    $.when(idcard(), phone()).done(function(a1,a2) {
        if(a1!=undefined){
            if(a1[0] == false){
                //提交调用校验
                if (doctor_add_flag) {
                    do_save_jy();
                }
            }
        }
        if(a2!=undefined){
            if(a2[0] == false){
                //提交调用校验
                if (doctor_add_flag) {
                    do_save_jy();
                }
            }
        }
    });
}
</script>
